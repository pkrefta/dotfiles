
export EDITOR='vim'
export TERM='screen-256color'

export PATH=/opt/X11/bin:$PATH

# no autocorrection
unsetopt correct_all
unsetopt correct
setopt no_hist_verify

export PYENV_VIRTUALENV_DISABLE_PROMPT=1

if which rbenv > /dev/null; then
	eval "$(rbenv init -)"
fi

export HOMEBREW_NO_AUTO_UPDATE="1"
